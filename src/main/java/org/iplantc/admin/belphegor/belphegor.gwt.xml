<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='belphegor'>
	<!-- Inherit the core Web Toolkit stuff. -->
	<inherits name='com.google.gwt.user.User' />

	<!-- Inherit the default GWT style sheet. You can change -->
	<!-- the theme of your GWT application by uncommenting -->
	<!-- any one of the following lines. -->
	<inherits name='com.google.gwt.user.theme.standard.Standard' />
	<!-- <inherits name='com.google.gwt.user.theme.chrome.Chrome'/> -->
	<!-- <inherits name='com.google.gwt.user.theme.dark.Dark'/> -->

	<!-- Other module inherits -->

	<inherits name="com.google.gwt.i18n.I18N" />
	<inherits name="com.google.gwt.resources.Resources" />
	<inherits name='com.extjs.gxt.ui.GXT' />
    <inherits name='com.sencha.gxt.ui.GXT' />
    <inherits name='com.sencha.gxt.theme.gray.Gray'/>
    
    <inherits name="com.google.gwt.uibinder.UiBinder" />
    
	<inherits name="org.iplantc.core.JsonUtil" />

	<inherits name='org.iplantc.de.DiscoveryEnvironmentCommon' />
	<inherits name="org.iplantc.core.ui-commons"/>
	<inherits name="org.iplantc.core.applications"/>
	<inherits name="org.iplantc.core.Widgets" />
	
	<!-- Specify the paths for translatable code -->
	<source path='client' />
	<source path='shared' />
   
   <!-- ui-applications deferred binding rules -->
   <replace-with class="org.iplantc.admin.belphegor.client.services.impl.AppAdminServiceFacade">
      <when-type-is class="org.iplantc.core.uiapplications.client.services.AppServiceFacade"/>
   </replace-with>
   <replace-with class="org.iplantc.admin.belphegor.client.services.impl.AppAdminUserServiceFacade">
      <when-type-is class="org.iplantc.core.uiapplications.client.services.AppUserServiceFacade"/>
   </replace-with>


	<!-- Specify the app entry point class. -->
	<entry-point class='org.iplantc.admin.belphegor.client.Belphegor' />
</module>
